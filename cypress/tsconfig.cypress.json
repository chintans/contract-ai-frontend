{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "types": [
            "cypress",
            "node",
            "@types/chai",
            "cypress-axe",
            "@testing-library/cypress",
            "cypress-real-events"
        ]
    },
    "include": [
        "**/*.cy.ts"
    ],
    "exclude": [
        "**/*.spec.ts",
        "**/*.d.ts",
        "../src/**/*.spec.ts",
        "../src/**/*.d.ts"
    ]
}